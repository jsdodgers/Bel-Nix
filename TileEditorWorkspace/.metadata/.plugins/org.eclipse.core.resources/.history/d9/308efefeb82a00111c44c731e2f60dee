
public class Interpreter
{
	
	ProgramState state;
	String[] list;
	
	public Interpreter (String[] strings) {
		list = strings;
		state = new ProgramState();
		state.addToArray(null)
	}
	
	
	public void parse() {
		for (int n=1;n<list.length;n++) {
			state.addToArray(Parser.parseLine(list[n]));
		}
	}
	
	
	public void go () {
		while (!state.isFinished()) {
			try {
				state.currentStatement().execute(state);
			}
			catch (FatalException e) {
				System.out.println(e.getReason());
				state.setFinished();
			}
		}
		
	}
	
	
}
