import java.util.Arrays;

public class TaxiBusiness {

	public int[] place(String[] town) {
		int numberOfThem = -1;
		int[] expected = new int[2];
		for (int n=0;n<town.length;n++) {
			for (int m=0;m<town[n].length();m++) {
				if (town[n].charAt(m)=='-') {
					int numberOfThem2 = 0;
					int distance2 = -1;
					for (int x=0;x<town.length;x++) {
						for (int y=0;y<town[x].length();y++) {
							if (town[x].charAt(y)=='-') {
								int distance = Math.abs(x-n) + Math.abs(y-m);
								int distance22 = -1;
								for (int a = 0;a<town.length;a++) {
									for (int b=0;b<town[a].length();b++) {
										if (town[a].charAt(b)=='T') {
											int distance23 = Math.abs(x-a) + Math.abs(y-b);
											if (distance23<distance22 || distance22 == -1) {
												distance22 = distance23;
											}
										}
									}
								}
								if (distance<distance22 || distance22==-1) {
									numberOfThem2++;
								}
							}
						}
					}
					if (numberOfThem2<numberOfThem || numberOfThem==-1) {
						numberOfThem = numberOfThem2;
						expected[0] = n;
						expected[1] = m;
					}
					else if(numberOfThem2 == numberOfThem) {
						if (n<expected[0]) {
							expected[0] = n;
							expected[1] = m;
						}
						else if(n==expected[0] && m<expected[1]) {
							expected[0] = n;
							expected[1] = m;
						}
					}
				}
			}
		}
		System.out.println(Arrays.toString(expected));
		return expected;
	}

}
